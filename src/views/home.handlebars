<div>
    <h1>Todos los productos:</h1>
    <div class="col-md-6 p-2 ">
            <div id="listaProds" class="container">
                {{#each products}}
                <div class="card text-bg-dark mb-3" style="max-width: 540px;">
                    <div class="row g-0">
                        <div class="col-md-4">
                            <img src="" class="img-fluid rounded-start" alt="">
                        </div>
                        <div class="col-md-8">
                            <div class="card-header">{{this.category}}</div>
                            <div class="card-body">
                                <h5 class="card-title">{{this.title}}</h5>
                                <strong>ID:</strong> {{this.id}}
                                <p class="card-text">{{this.description}}</p>
                                <strong>Precio:</strong> {{this.price}}
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
    </div>
</div>